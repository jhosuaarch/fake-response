{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php@0.6.0",
      "config": {
        "maxDuration": 10,
        "includeFiles": "api/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php",
      "headers": {
        "Content-Type": "text/html"
      }
    }
  ]
}
